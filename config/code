CREATE TABLE "cleaning_platform"."business_account" (
  "id" varchar(255) COLLATE "pg_catalog"."default" NOT NULL,
  "avatar" varchar(255) COLLATE "pg_catalog"."default",
  "business_id" varchar(255) COLLATE "pg_catalog"."default",
  "created_time" int8,
  "deleted" bool NOT NULL,
  "description" varchar(255) COLLATE "pg_catalog"."default",
  "disabled" bool NOT NULL,
  "email" varchar(255) COLLATE "pg_catalog"."default",
  "mobile" varchar(255) COLLATE "pg_catalog"."default",
  "name" varchar(255) COLLATE "pg_catalog"."default",
  "require_change_password" bool NOT NULL,
  "username" varchar(255) COLLATE "pg_catalog"."default",
  CONSTRAINT "business_account_pkey" PRIMARY KEY ("id")
)
;

ALTER TABLE "cleaning_platform"."business_account" 
  OWNER TO "ctirobot";